<header>
  <nav>
    <a href="/">
      <img
        src="/images/logo.png"
        alt="Stolabase - Go to homepage"
        class="logo"
      />
    </a>
    <ul>
      <li class="search">
        <button type="button">
          <img src="/icons/search-white.svg" alt="search" />
        </button>
        <input type="text" placeholder="Search" />
      </li>
      <% links.forEach(link => { %>
      <li><a href="<%= link.url %>"><%= link.name %></a></li>
      <% }) %>
    </ul>
  </nav>
</header>
